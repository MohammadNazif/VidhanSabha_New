<style>

    .dataTables_length select {
        font-size: 12px !important;
        padding: 2px 5px !important;
        width: 54px !important;
        height: 28px !important;
    }

    .dataTables_filter input {
        font-size: 12px !important;
        padding: 3px 6px !important;
        width: 150px !important;
        height: 28px !important;
        border-radius: 4px;
    }

    div.dataTables_filter input[type="search"] {
        border: 1px solid #FF8C42 !important;
    }

    #myTable_paginate .paginate_button {
        font-size: 12px; /* font size chhota ya bada karne ke liye */
        margin-top: 10px;
        padding: 5px 10px; /* button ke andar ki padding */
        min-width: 40px; /* button ki minimum width */
        height: 30px; /* button ki height */
        border-radius: 5px; /* corners round karne ke liye */
        margin: 0 2px; /* buttons ke beech thodi gap */
        cursor: pointer;
        background-color: #FF8C42;
        color: black;
        border: none;
    }

    #myTable td {
        padding: 2px 6px !important; /* kam padding = chhoti row */
        line-height: 1.2; /* text gap kam */
        font-size: 13px; /* optional: chhota font */
    }

        #myTable th.coll-col,
        #myTable td.coll-col {
            width: 50px; /* jitna chhota chahiye utna set karo */
            white-space: nowrap; /* text wrap na ho */
            text-align: center; /* text center/right/left jaisa chahiye */
        }

        #myTable th.caste-col,
        #myTable td.caste-col {
            width: 110px;
            white-space: nowrap;
            text-align: left; /* left align */
        }

    .table-responsive {
        overflow-x: auto;
        white-space: nowrap;
        max-height: 80vh !important;
    }

    .table-hover tbody tr td svg {
        width: 15px;
        padding: 1px;
        border-radius: 0;
        margin-right: 0;
    }

    .btn-lg {
        --bs-btn-padding-y: 0.5px;
    }

    .round-0 {
        border-radius: 5px !important;
    }
</style>

