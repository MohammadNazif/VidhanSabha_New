<style>

    .sidebar-list {
        text-decoration: none;
    }

    body {
        background-color: #fdfaf6;
        font-family: 'Poppins', sans-serif;
    }

    .sidebar-list a {
        text-decoration: none; /* Removes underline */
        color: inherit; /* Keeps the original color */
    }


        .sidebar-list a:hover {
            text-decoration: none;
        }

    .sidebar-list {
        margin-left: -2px;
    }

    .sidebar-wrapper {
        width: 240px; /* default width */
        transition: all 0.3s ease-in-out;
    }

        .sidebar-wrapper.closed {
            width: 0;
            overflow: hidden;
        }
    /* Sidebar default open */
    .sidebar-wrapper {
        width: 250px;
        transition: all 0.3s ease;
        overflow: hidden;
    }

        /* Sidebar closed */
        .sidebar-wrapper.closed {
            width: 60px !important; /* force close */
            overflow: hidden;
        }

        /* --- prevent hover auto open --- */
        .sidebar-wrapper:hover {
            width: inherit; /* disable auto width change */
        }

    /* Sidebar open */
    .sidebar-wrapper {
        width: 250px;
        transition: all 0.3s ease;
        overflow: hidden;
        position: relative;
    }

        /* Sidebar closed (sirf toggle button ke liye chhota space) */
        .sidebar-wrapper.closed {
            width: 60px; /* jitna bars / cross dikh sake utna */
            overflow: hidden;
        }
    /* icon hamesha visible rahe */
    .toggle-sidebar {
        position: relative;
/*        buttom: 10px;*/
/*        right: 0px;*/
        z-index: 10000;
        cursor: pointer;
        transition: all 0.3s ease;
        /*            color:red;*/
    }

        .toggle-sidebar i {
            font-size: 22px;
            color: black;
            display: inline-block !important;
            opacity: 1 !important;
            visibility: visible !important;
        }

    /* agar kahin CSS me sidebar close pe hide kar raha hai to force override */
    .sidebar-wrapper.closed .toggle-sidebar i {
        opacity: 1 !important;
        visibility: visible !important;
        display: inline-block !important;
    }



    .dataTables_length select {
        font-size: 12px !important;
        padding: 2px 5px !important;
        width: 54px !important;
        height: 28px !important;
    }

    .dataTables_filter input {
        font-size: 12px !important;
        padding: 3px 6px !important;
        width: 150px !important;
        height: 28px !important;
        border-radius: 4px;
    }

    div.dataTables_filter input[type="search"] {
        border: 1px solid #FF8C42 !important;
    }

    #myTable_paginate .paginate_button {
        font-size: 12px; /* font size chhota ya bada karne ke liye */
        margin-top: 10px;
        padding: 5px 10px; /* button ke andar ki padding */
        min-width: 40px; /* button ki minimum width */
        height: 30px; /* button ki height */
        border-radius: 5px; /* corners round karne ke liye */
        margin: 0 2px; /* buttons ke beech thodi gap */
        cursor: pointer;
        background-color: #FF8C42;
        color: black;
        border: none;
    }

    #myTable td {
        padding: 2px 6px !important; /* kam padding = chhoti row */
        line-height: 1.2; /* text gap kam */
        font-size: 13px; /* optional: chhota font */
    }

        #myTable th.coll-col,
        #myTable td.coll-col {
            width: 50px; /* jitna chhota chahiye utna set karo */
            white-space: nowrap; /* text wrap na ho */
            text-align: center; /* text center/right/left jaisa chahiye */
        }

        #myTable th.caste-col,
        #myTable td.caste-col {
            width: 110px;
            white-space: nowrap;
            text-align: left; /* left align */
        }

    .table-responsive {
        overflow-x: auto;
        white-space: nowrap;
        max-height: 80vh !important;
    }

    .table-hover tbody tr td svg {
        width: 15px;
        padding: 1px;
        border-radius: 0;
        margin-right: 0;
    }

    .btn-lg {
        --bs-btn-padding-y: 0.5px;
    }


</style>


