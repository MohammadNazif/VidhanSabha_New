                $.ajax({
                    url: '/Admin/GetSubcasteByCategory',
                    type: 'GET',
                    data: { id: selectedId },
                    success: function (response) {

                        $("#Caste").empty();
                        $("#Caste").append('<option value="">-- Select Caste --</option>');


                        $.each(response, function (i, item) {
                            $("#Caste").append('<option value="' + item.SubCaste_Id + '">' + item.SubCasteName + '</option>');
                        });
                    },
                    error: function () {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Some error occured!!'
                        });
                    }
                });
            } else {
                $("#Caste").empty();
                $("#Caste").append('<option value="">-- Select Caste --</option>');
            }
        });
    });
</script>
<script>
    $(document).ready(function () {
    $('#Booth_Id').on('change', function () {
    const boothId = $(this).val();
    if (!boothId) return;

    $.ajax({
        url: '@Url.Action("GetVillagesByBoothId", "Admin")',
        type: 'GET',
        data: { boothId },
        success: function (villages) {
            const $villageList = $('#VillageList');
            $villageList.empty();
            villages.forEach(v => {
                $villageList.append(`<option value="${v.villageId}">${v.Villagename}</option>`);
            });

            // Re-apply previously selected villages if editing
            if (window.selectedVillages && window.selectedVillages.length > 0) {
                $villageList.val(window.selectedVillages).trigger('change');
            }
        }
    });
});



    });
</script>


