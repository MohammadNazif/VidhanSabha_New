<script>

    // Load table data
$(document).ready(function () {

    $('#boothFilter').on('change', function () {
        let selected = $(this).val(); // array return karega
        if (selected && selected.includes("all")) {
            loadPannaPramukh(["all"]);
        } else if (selected && selected.length > 0) {
            loadPannaPramukh(selected);
        } else {
            loadPannaPramukh(["all"]);
        }
    });

    function loadPannaPramukh(selectedBooth) {
        let ajaxOptions = {
            url: '@Url.Action("GetAllPannaPramukh", "Admin")',
            type: 'GET',
            traditional: true,
            success: function (response) {
                const data = Array.isArray(response) ? response : response.data;
                const tbody = $('#myTable tbody').empty();

                let i = 0;
                data.forEach(item => {
                    const imgHtml = item.ProfileImageUrl
                        ? `<img src="${item.ProfileImageUrl}" height="40" width="40" onerror="this.style.display='none'" />`
                        : '';
                    tbody.append(`
                        <tr class="text-center align-middle" id="row_${item.PannaPramukh_Id}">
                            <td style="width:10px;text-align:center!important;">${++i}</td>
                            <td style="text-align:center!important;">${item.BoothNumber || '-'}</td>
                            <td style="width:20px">${item.village || '-'}</td>
                       @*<td style="width:20px">
        <a href="https://www.google.com/maps/search/?api=1&query=${encodeURIComponent(item.location || '-')}"
           target="_blank" rel="noopener noreferrer" title="Open in Google Maps">
            <i class="fa fa-map-marker-alt" style="font-size:18px;"></i>
        </a>
    </td>*@
                            @*<td style="width:20px">${item.location || '-'}</td>*@
                            <td class="data-left">${item.Pannapramukh || '-'}</td>
                            <td>${item.PannaNumber || '-'}</td>
                            <td class="data-left">${item.SubCasteName || '-'}</td>
                            <td>${item.VoterNumber || '-'}</td>
                            <td class="data-left">${item.Address || '-'}</td>
                            <td class="data-left">${item.Mobile || '-'}</td>
                            <td >${imgHtml}</td>
                            <td>
                                <button type="button" class="btn btn-xs me-1 edit-btn" data-id="${item.PannaPramukh_Id}" data-bs-toggle="modal" data-bs-target="#createPannaPramukhModal" title="Edit" style="font-size:8px!important;">
                                    <i class="fas fa-edit edit-icon"></i>
                                </button>
                                <button type="button" class="btn btn-xs me-1 delete-btn" data-id="${item.PannaPramukh_Id}" title="Delete" style="font-size:8px!important;">
                                    <i class="fas fa-trash-alt delete-icon"></i>
                                </button>
                            </td>
                        </tr>`);
                });
            },
            error: function () {
                Swal.fire('Error', 'Failed to load Panna Pramukh data.', 'error');
            }
        };

        // 🔹 Agar filter se call hai toh parameter bhejna, warna nahi
        if (selectedBooth) {
            ajaxOptions.data = { boothNumbers: selectedBooth };
        }

        $.ajax(ajaxOptions);
    }

    // 🔹 Page load par bina parameter ke saara data load hoga
    loadPannaPramukh();
});






    // Reset form on Create
    $('#openCreateModal').on('click', function () {
        $('#pannaPramukhForm')[0].reset();
        $('#PannaPramukh_Id').val('');
        $('#createPannaPramukhLabel').text('Create New Panna Pramukh');
