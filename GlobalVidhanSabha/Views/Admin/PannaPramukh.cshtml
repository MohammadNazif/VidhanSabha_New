        // Populate form for Edit

 $(document).on('click', '.edit-btn', function () {
    const id = $(this).data('id');

    $.ajax({
        url: '@Url.Action("GetAllPannaPramukh", "Admin")',
        type: 'GET',
        success: function (response) {
            console.log(response,"Panna Data")
            const data = Array.isArray(response) ? response : response.data;
            const item = data.find(x => x.PannaPramukh_Id === id);
            if (!item) return Swal.fire('Error', 'Record not found.', 'error');

            // ✅ Basic info
            $('#PannaPramukh_Id').val(item.PannaPramukh_Id);
            $('[name="Pannapramukh"]').val(item.Pannapramukh);
            $('[name="PannaNumber"]').val(item.PannaNumber);
            $('[name="PannaNumTo"]').val(item.PannaNumTo);
            $('[name="VoterNumber"]').val(item.VoterNumber);
            $('[name="Address"]').val(item.Address);
            $('[name="Mobile"]').val(item.Mobile);
            $('#createPannaPramukhLabel').text('Update Panna Pramukh');

            // ✅ Category and caste
            $('#Category').val(item.Category).trigger('change');
            setTimeout(() => {
                $('#Caste').val(item.Cast).trigger('change');
            }, 500);

            // ✅ Profile Image (if applicable)
            if (item.ProfileImageUrl) {
                $('#previewImage').attr('src', item.ProfileImageUrl).show();
            }

            // ✅ Booth
            $('#Booth_Id').val(item.Booth_Id).trigger('change');

            // ✅ Store selected villages temporarily
            window.selectedVillages = item.VillageListId
                ? item.VillageListId.split(',').map(v => v.trim())
                : [];

            // ✅ Wait for Booth change AJAX to load villages, then set selected ones
            const waitForVillageList = setInterval(() => {
                const $villageList = $('#VillageList');
                if ($villageList.find('option').length > 0) {
                    $villageList.val(window.selectedVillages).trigger('change');
                    clearInterval(waitForVillageList);
                }
            }, 300);

            // ✅ Optional: Display readable village names somewhere
            if (item.village) {
                $('#SelectedVillageNames').text(item.village);
                // (Make sure to have a span or div with this ID if you want to show names)
            }
        },
        error: function () {
            Swal.fire('Error', 'Failed to fetch record.', 'error');
        }
    });
});





    // Delete button click handler (soft delete)
$(document).on('click', '.delete-btn', function () {
    const id = $(this).data('id');
    const row = $(this).closest('tr');

    Swal.fire({
        title: 'Are you sure?',
        text: "This will soft delete the record.",
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#d33',
        cancelButtonColor: '#3085d6',
        confirmButtonText: 'Yes, delete it!'
    }).then((result) => {
        if (result.isConfirmed) {
            $.ajax({
                url: '@Url.Action("DeletePannaPramukh", "Admin")',
                type: 'POST',
                data: { id: id },
                success: function (response) {
                    if (response.success) {
                        Swal.fire('Deleted!', response.message, 'success');
                        row.remove();
                        loadPannaPramukh();  // refresh table after delete
                    } else {
                        Swal.fire('Error', response.message, 'error');
                    }
                },
                error: function () {
                    Swal.fire('Error', 'Failed to delete the record.', 'error');
                }
            });
        }
    });
});


        // Submit form for both create & update
        $('#pannaPramukhForm').on('submit', function (e) {
    e.preventDefault();

    const formData = new FormData(this);

    // ✅ Get all selected village IDs
    const selectedVillages = $('#VillageList').val(); // returns array of selected values

    if (selectedVillages && selectedVillages.length > 0) {
        // Convert array to comma-separated string
        const villageIds = selectedVillages.join(',');
        formData.set('VillageListId', villageIds); // override with comma-separated value
    } else {
        formData.set('VillageListId', ''); // no selection
    }

    console.log("Village IDs:", formData.get('VillageListId'));

    $.ajax({
        url: '@Url.Action("Pannapramukh", "Admin")',
        type: 'POST',
        data: formData,
        processData: false,
        contentType: false,
        success: function (response) {
            console.log("Response from server:", response);

            if (response.success) {
                Swal.fire('Success', response.message || 'Saved successfully!', 'success').then(() => {
                    location.reload();
                });

                const modalEl = document.getElementById('createPannaPramukhModal');
                const modal = bootstrap.Modal.getInstance(modalEl);
                if (modal) modal.hide();

                $('body').removeClass('modal-open');
                $('.modal-backdrop').remove();
                $('#pannaPramukhForm')[0].reset();
                $('#PannaPramukh_Id').val('');
                loadPannaPramukh();
            } else {
                Swal.fire('Error', response.message || 'Failed to save.', 'error');
            }
        },
        error: function () {
            Swal.fire('Error', 'An unexpected error occurred.', 'error');
        }
    });
});




